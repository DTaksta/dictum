            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "24.0.13",
        "@types/node": "12.0.4",
        "@types/react": "16.8.19",
        "@types/react-dom": "16.8.4",
        "@types/react-router-config": "^5.0.0",
        "@types/react-router-dom": "^4.3.3",
        "husky": "^2.3.0",
        "lint-staged": "^8.1.7",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.1",
        "pretty-quick": "^1.11.0",
        "tslint": "^5.17.0"
    }
}
