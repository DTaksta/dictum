{
    "name": "quote-web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-config": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-scripts": "3.0.1",
        "typescript": "3.5.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "NODE_ENV=production lint-staged"
        }
